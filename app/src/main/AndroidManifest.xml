<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.se.onlinequizsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineQuizSystem">
        <activity
            android:name=".InstructorMainMenuActivity"
            android:parentActivityName=".Login" />
        <activity
            android:name=".InstructorQuizGradeActivity"
            android:parentActivityName=".InstructorQuizListActivity" />
        <activity
            android:name=".InstructorQuizGenerationActivity"
            android:parentActivityName=".InstructorMainMenuActivity" />
        <activity android:name=".Login" />
        <activity
            android:name=".InstructorAddEditQuestionActivity"
            android:parentActivityName=".InstructorQuestionBankListActivity" />
        <activity
            android:name=".InstructorQuizListActivity"
            android:parentActivityName=".InstructorMainMenuActivity" />
        <activity
            android:name=".InstructorQuestionBankListActivity"
            android:parentActivityName=".InstructorMainMenuActivity" />
        <activity
            android:name=".InstructorQuizFeedbackActivity"
            android:parentActivityName=".InstructorQuizListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>